{
	"name": "david63/acpusernotifications",
	"type": "phpbb-extension",
	"description": "Adds an option to the ACP Manage users to edit a user's notifications.",
	"version": "3.1.0-RC1",
	"time": "2020-07-30",
	"keywords": [
		"ACP",
		"version",
		"check"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Developer"
		}],
	"require": {
        "php": ">=7.1.0",
		"phpbb/phpbb": ">=3.3.0",
        "composer/installers": "~1.0"
    },
	"extra": {
        "display-name": "ACP user notifications",
        "soft-require": {
            "phpbb/phpbb": ">=3.3.0"
        },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "acp_usernotifications_version_file.json",
			"ssl": false
    	}
	}
}
